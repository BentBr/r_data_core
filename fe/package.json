{
    "name": "r-data-core-admin",
    "description": "Admin FE for r_data_core, the MDM with pipelines and easy data orchestration.",
    "version": "0.4.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "npm run generate:favicons && vue-tsc && vite build",
        "preview": "vite preview",
        "typecheck": "vue-tsc --noEmit",
        "lint": "vue-tsc --noEmit && eslint . --fix",
        "format": "prettier --write src/",
        "test": "vue-tsc --noEmit && vitest",
        "test:ui": "vitest --ui",
        "test:run": "vue-tsc --noEmit && vitest run",
        "test:coverage": "vue-tsc --noEmit && vitest run --coverage",
        "generate:favicons": "node scripts/generate-favicons.js"
    },
    "dependencies": {
        "@vueuse/core": "^10.9.0",
        "lucide-vue-next": "^0.556.0",
        "pinia": "^2.1.7",
        "vue": "^3.4.21",
        "vue-router": "^4.3.0",
        "vuetify": "^3.5.14",
        "zod": "^4.0.13"
    },
    "devDependencies": {
        "sharp": "^0.33.5",
        "@eslint/js": "^9.9.0",
        "@types/node": "^20.11.30",
        "@typescript-eslint/eslint-plugin": "^8.38.0",
        "@typescript-eslint/parser": "^8.38.0",
        "@vitejs/plugin-vue": "^5.0.4",
        "@vitest/coverage-v8": "^3.2.4",
        "@vitest/ui": "^3.2.4",
        "@vue/test-utils": "^2.4.0-alpha.2",
        "eslint": "^9.9.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-vue": "^9.27.0",
        "jsdom": "^26.1.0",
        "prettier": "^3.2.5",
        "typescript": "~5.4.3",
        "vite": "^6.3.5",
        "vite-plugin-vuetify": "^2.1.2",
        "vitest": "^3.2.4",
        "vue-eslint-parser": "^9.4.3",
        "vue-tsc": "^2.0.7"
    }
}
