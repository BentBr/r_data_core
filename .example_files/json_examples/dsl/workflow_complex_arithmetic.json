{
  "steps": [
    {
      "from": {
        "type": "format",
        "source": {
          "source_type": "uri",
          "config": {
            "uri": "http://example.com/data.csv"
          }
        },
        "format": {
          "format_type": "csv",
          "options": {
            "header": true,
            "delimiter": ","
          }
        },
        "mapping": {
          "source_col1": "normalized_field1",
          "source_col2": "normalized_field2"
        }
      },
      "transform": {
        "type": "arithmetic",
        "target": "total",
        "left": {
          "kind": "field",
          "field": "normalized_field1"
        },
        "op": "add",
        "right": {
          "kind": "const",
          "value": 5.0
        }
      },
      "to": {
        "type": "format",
        "output": {
          "mode": "api"
        },
        "format": {
          "format_type": "json",
          "options": {}
        },
        "mapping": {
          "normalized_field1": "output_field1",
          "normalized_field2": "output_field2",
          "total": "total"
        }
      }
    }
  ]
}

