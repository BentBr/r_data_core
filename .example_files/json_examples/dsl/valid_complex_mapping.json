{
  "steps": [
    {
      "from": {
        "type": "csv",
        "uri": "http://example.com/data.csv",
        "options": {
          "header": true,
          "delimiter": ",",
          "quote": "\""
        },
        "mapping": {
          "source_col1": "normalized_field1",
          "source_col2": "normalized_field2",
          "source_col3": "normalized_field3",
          "nested.source": "nested.target"
        }
      },
      "transform": {
        "type": "none"
      },
      "to": {
        "type": "json",
        "output": "api",
        "mapping": {
          "output_field1": "normalized_field1",
          "output_field2": "normalized_field2",
          "output_field3": "normalized_field3",
          "final.nested": "nested.target"
        }
      }
    }
  ]
}

