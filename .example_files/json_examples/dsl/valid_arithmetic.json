{
  "steps": [
    {
      "from": {
        "type": "format",
        "source": {
          "source_type": "uri",
          "config": {
            "uri": "http://example.com/data.csv"
          },
          "auth": null
        },
        "format": {
          "format_type": "csv",
          "options": {}
        },
        "mapping": {
          "price": "price"
        }
      },
      "transform": {
        "type": "arithmetic",
        "target": "price",
        "left": { "kind": "field", "field": "price" },
        "op": "add",
        "right": { "kind": "const", "value": 5.0 }
      },
      "to": {
        "type": "format",
        "output": {
          "mode": "api"
        },
        "format": {
          "format_type": "json",
          "options": {}
        },
        "mapping": {
          "entity.total": "price"
        }
      }
    }
  ]
}


