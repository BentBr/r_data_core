{
  "steps": [
    {
      "from": {
        "type": "format",
        "source": {
          "source_type": "uri",
          "config": {
            "uri": "http://example.com/products.csv"
          },
          "auth": null
        },
        "format": {
          "format_type": "csv",
          "options": {
            "has_header": true,
            "delimiter": ","
          }
        },
        "mapping": {
          "product_name": "name",
          "product_price": "price",
          "product_category": "category"
        }
      },
      "transform": {
        "type": "arithmetic",
        "target": "price_with_tax",
        "left": {
          "kind": "field",
          "field": "price"
        },
        "op": "mul",
        "right": {
          "kind": "const",
          "value": 1.19
        }
      },
      "to": {
        "type": "entity",
        "entity_definition": "product",
        "path": "/products",
        "mode": "create",
        "mapping": {
          "name": "name",
          "price": "price",
          "price_with_tax": "price_with_tax",
          "category": "category"
        }
      }
    }
  ]
}

