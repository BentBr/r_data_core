{
  "steps": [
    {
      "from": {
        "type": "csv",
        "uri": "http://example.com/data.csv",
        "mapping": {
          "price": "price"
        }
      },
      "transform": {
        "type": "arithmetic",
        "target": "total",
        "left": {
          "kind": "field",
          "field": "nonexistent_field"
        },
        "op": "add",
        "right": {
          "kind": "const",
          "value": 5.0
        }
      },
      "to": {
        "type": "json",
        "output": "api",
        "mapping": {
          "total": "total"
        }
      }
    }
  ]
}

